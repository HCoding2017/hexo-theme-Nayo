
<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8">
  <meta name="viewport"content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=yes"/>

  <%  var title = page.title;
  if (is_archive()){
    title = 'Archives';
    if (is_month()){
      title += ': ' + page.year + '/' + page.month;
    } else if (is_year()){
      title += ': ' + page.year;
    }
  } else if (is_category()){
    title = 'Category: ' + page.category;
  } else if (is_tag()){
    title = 'Tag: ' + page.tag;
  }  
  %>
  
  <title><% if (title){ %><%= title %> | <% } %><%= theme.profile.title %></title>
 
  <% if (theme.favicon){ %>
    <link rel="icon" href="<%- theme.favicon %>">
  <% } %>


  <%- css('css/style.css') %>  

  <%- js('//cdn.staticfile.org/jquery/3.2.1/jquery.min.js') %> 



  <!-- Add fancyBox -->
  <%- css('https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.1.25/jquery.fancybox.min.css') %>
  <%- js('https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.1.25/jquery.fancybox.min.js') %> 
  <%- js('js/pic') %>


  <%- js('js/script.js') %> 

</head>